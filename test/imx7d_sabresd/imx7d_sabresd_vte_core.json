
{
    "health_check": false,
    "logging_level": "DEBUG",
    "job_name": "imx7d_sabresd_vte_core",
    "target": "imx7d-sabresd",
    "timeout": 3000,
    "actions": [
        {
            "command": "deploy_linaro_kernel",
            "parameters": {
                "kernel": "file:///nfsroot/imx7dsabresdcore/image/zImage-imx6ul7d.bin",
                "dtb": "file:///nfsroot/imx7dsabresdcore/image/imx7d-sdb.dtb",
		"nfsrootfs": "file:///nfsroot/imx7dsabresdcore/image/imx7dsabresd.tar",
                "login_prompt": "imx6ul7d login:",
                "username": "root"
            }
        },
        {
            "command": "boot_linaro_image",
            "parameters": {
                "test_image_prompt": "root@imx6ul7d:~#"
            }
        },
	{
	    "command": "lava_test_shell",
	    "parameters": {
		"testdef_repos": [
		    {
			"git-repo": "https://github.com/eslover/sandbox-test.git",
			"testdef": "lava_vte_mx7d_core.yaml"
		    }
		],
		"timeout": 86400
	    }
	},
	{
            "command": "submit_results_on_host",
            "parameters": {
                "server": "http://10.192.242.176/RPC2/",
                "stream": "/anonymous/admin/"
            }
        }
    ]
}
